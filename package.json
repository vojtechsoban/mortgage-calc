{
  "name": "mortgage-calc",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading",
  "scripts": {
    "start": "cross-env NODE_ENV=dev webpack-dev-server --config webpack.config.dev.js",
    "dist": "BABEL_ENV=prod webpack --config webpack.config.prod.js --progress -p",
    "lint": "eslint src",
    "test": "NODE_ENV=test BABEL_ENV=test nyc mocha ./src/**/*.spec.js ./test/**/*.spec.js -r babel-register -r ./test/setup.js --recursive ",
    "test:watch": "npm test -- --watch"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "tempDirectory": "./artifacts/coverage/.nyc_output",
    "extension": [
      ".jsx"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./artifacts/coverage",
    "exclude": [
      "**/*.spec.js",
      "dist",
      "test/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vojtechsoban/mortgage-calc.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Vojtech Soban <vojtech.soban@gmail.com> (http://github.com/vojtechsoban)",
  "license": "MIT",
  "homepage": "https://github.com/vojtechsoban/mortgage-calc",
  "devDependencies": {
    "app-module-path": "^2.2.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
    "dash-assert": "^1.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.29.0",
    "jsdom": "^16.5.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "rewire": "^3.0.2",
    "sinon": "^4.2.2",
    "sinon-chai": "^2.11.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "node-sass": "^4.5.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.0",
    "prop-types": "^15.5.10",
    "react": "^16.2.0",
    "react-datetime": "^2.9.0",
    "react-dom": "^16.2.0",
    "react-fa": "^5.0.0",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "redux-form": "^7.2.3",
    "sass-loader": "^6.0.3",
    "semantic-ui-css": "^2.2.11",
    "semantic-ui-react": "^0.78.2",
    "style-loader": "^0.20.1"
  }
}
