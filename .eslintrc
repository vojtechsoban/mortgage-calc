{
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "semi": [2, "always"],
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "no-unused-vars": "warn",
    "no-console": "off",
    "react/prop-types": "warn"
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "Symbol": false,
    "window": false,
    "process": false,
    "require": false,
    "module": false,
    "__dirname": false
  }
}
